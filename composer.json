{
    "name": "baywa-re-lusy/user-management",
    "description": "BayWa r.e. LUSY User Management",
    "type": "library",
    "license": "proprietary",
    "homepage": "https://github.com/baywa-re-lusy/user-management",
    "require": {
        "php": ">= 8.1",
        "laminas/laminas-permissions-acl": "^2.10",
        "baywa-re-lusy/jwt-authentication": "dev-feature/missing-fields"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.7",
